{
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "/src/.*binance/user.*\\.(test|spec).(ts)$",
  "testURL": "http://localhost/",
  "collectCoverageFrom": [
    "src/**/*.{ts}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageReporters": [
    "json",
    "lcov"
  ],
  "verbose": true
}
