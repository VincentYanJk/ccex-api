{
  "name": "ccex-api",
  "version": "0.0.3",
  "description": "Crypto currencies exchanges api implementation (include realtime api)",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --config .prettierrc --write src/**/*.ts",
    "lint": "tslint -c tslint.json --project tsconfig.json \"src/**/*.ts\"",
    "test": "mocha -r ts-node/register ./src/**/*.spec.ts",
    "tests-main": "gulp tests-main",
    "tests-sub": "gulp tests-sub"
  },
  "keywords": [
    "crypto",
    "cryptocurrencies",
    "exchange"
  ],
  "repository": "git@github.com:dang1412/ccex-api.git",
  "author": "Dang Thanh Tung",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.1.2",
    "pubnub": "^4.20.3",
    "rxjs": "^6.0.0",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/gulp": "^4.0.5",
    "@types/gulp-mocha": "0.0.32",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.1.2",
    "@types/node-fetch": "^1.6.9",
    "@types/pubnub": "^4.0.2",
    "@types/ws": "^5.1.1",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-mocha": "^6.0.0",
    "mocha": "^5.2.0",
    "prettier": "1.13.7",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.9.2"
  }
}
